9 timeunits:      php artisan crud:generate Timeunits --fields_from_file="app/Http/Controllers/CrudGen/timeunits.json" --view-path=manager --controller-namespace=Manager --route-group=manager
10 workergroups:  php artisan crud:generate Workergroups --fields_from_file="app/Http/Controllers/CrudGen/workergroups.json" --view-path=manager --controller-namespace=Manager --route-group=manager
11 workers:       php artisan crud:generate Workers --fields_from_file="app/Http/Controllers/CrudGen/workers.json" --view-path=manager --controller-namespace=Manager --route-group=manager
